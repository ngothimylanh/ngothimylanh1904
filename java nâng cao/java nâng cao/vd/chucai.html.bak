<Script Language="JavaScript">
	function chuhoa()
	{
			var st=document.f1.t1.value
			document.f1.t2.value=st.toUpperCase()
	}

		</Script>
	<Body onLoad="document.f1.t1.focus()">
	    <Form name=f1 action=""method="">
			<input type=text name=t1 onBlur="chuhoa()">
			<input type=text name=t2>
	</Form>
</Body>

